<html>
    <head>
<style>
table,th,td {
    padding:10px;
    border: 1px solid black;
    border-collapse:collapse;
    }
    </style>
</head>
<body>
    <h1> Go, get your data!</h1>
    <br>
    <table>
        <thread>
            <tr>
                <th> Job ID</th>
                <th> Job Name</th>
                <th> Qty</th>
            </tr>
        </thread>
        <tbody>
                <% jobs.forEach(function(job){ %>
                    <tr>
                        <td> <%= job.jobID %></td>
                        <td> <%= job.jobName %></td>
                        <td> <%= job.qty %></td>
                    </tr>
                    <% }) %>
                </tbody>
            </table>
        </body>
        <br>
        <form action="/">
        <button>Home</button>
    </form>
    <br>
    <form action="/addData">
        <button>Add Data</button>
        </form>
        <form action="/updateData">
            <button>Update Data</button>
            </form>
        </html>